APP_ENV=testing
APP_DEBUG=true
APP_KEY=base64:YOUR_APP_KEY_HERE

DB_CONNECTION=sqlite
DB_DATABASE=:memory:

CACHE_DRIVER=array
SESSION_DRIVER=array
QUEUE_CONNECTION=sync

